  "version": "1.0.0",
  "description": "Enhanced MCP Server for Universal AI Expert Orchestration System",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp package.json dist/",
    "docker:build": "docker build -t universal-ai-mcp-server .",
    "docker:run": "docker run -p 3001:3001 universal-ai-mcp-server",
    "docker:compose": "docker-compose up -d"
  },
  "keywords": [
    "mcp",
    "ai-agents",
    "orchestration",
